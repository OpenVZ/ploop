.TH ploop 8 "26 May 2014" "OpenVZ" "Containers"
.SH NAME
ploop \- ploop device management utility
.SH SYNOPSYS
.SY ploop\ init
.B -s
.I size
.OP -f format
.OP -v version
.OP -t fstype
.OP -b blocksize
.OP -B fsblocksize
.OP --nolazy
.I delta_file
.YS
.SY ploop\ mount
.OP -r
.OP -F
.OP -f format
.OP -b blocksize
.OP -d device
.OP -m mount_point
.OP -o mount_options
.OP -t fstype
.I base_delta
.RI [ .\|.\|.
.IR top_delta ]
.YS
.SY ploop\ mount
.OP -r
.OP -F
.OP -d device
.OP -m mount_point
.OP -o mount_options
.OP -t fstype
.OP -u uuid\fR | \fBbase\fR
.\" .OP -c component
